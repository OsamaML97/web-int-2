            <div class="col-sm-10">
                {{ form_widget(form.lieuNaissance, {'attr': {'class': 'form-control'}}) }}
            </div>
        </div>



        <div class="form-group">
            {{ form_label(form.medicin, "medecin", {'label_attr': {'class': 'col-sm-2 control-label'}}) }}
            {{ form_errors(form.medicin) }}
            <div class="col-sm-10">
                {{ form_widget(form.medicin, {'attr': {'class': 'form-control'}}) }}
            </div>
        </div>



        <div class="form-group">
            {{ form_label(form.medecinNumero, "medecin numero", {'label_attr': {'class': 'col-sm-2 control-label'}}) }}
            {{ form_errors(form.medecinNumero) }}
            <div class="col-sm-10">
                {{ form_widget(form.medecinNumero, {'attr': {'class': 'form-control'}}) }}
            </div>
        </div>







        <div class="col-sm-10">
            {{ form_widget(form.Valider, {'attr': {'class': 'btn btn-primary'}}) }}
        </div>


        {{ form_rest(form) }}

        {{ form_end(form) }}
    </div>

</main>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
